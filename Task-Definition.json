{
  "networkMode": "bridge",
  "containerDefinitions": [
    {
      "memory": 1024,
      "portMappings": [
        {
          "containerPort": 8080,
          "protocol": "tcp"
        }
      ],
      "essential": true,
      "name": "%TASK_NAME",
      "image": "%IMAGE_ID",
      "cpu": 2,
      "memoryReservation": 512,
      "environment": [
        {
          "name": "JVM_ARGS",
          "value": "-Dserver.port=8080"
        },
        {
          "name" :  "AWS_ENV",
          "value": "%ENVIRONMENT_VALUE"
        }
      ]
    }
  ],
  "family": "%FAMILY_NAME"
}
